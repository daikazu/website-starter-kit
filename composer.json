{
    "name": "daikazu/website-starter-kit",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "repositories": {},
    "require": {
        "php": "^8.3",
        "ext-dom": "*",
        "ext-zip": "*",
        "blade-ui-kit/blade-heroicons": "^2.6",
        "blade-ui-kit/blade-icons": "^1.8",
        "daikazu/misc-helpers": "^1.2.0",
        "daikazu/sitemap": "^1.0.1",
        "daikazu/social-links": "^1.1",
        "daikazu/welcome-bar": "^1.1.3",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "graham-campbell/markdown": "^16.0",
        "illuminatech/url-trailing-slash": ">=1.1.14",
        "jaybizzle/crawler-detect": "^1.3.5",
        "laravel/framework": "^12.20",
        "laravel/tinker": "^2.10.1",
        "league/flysystem-aws-s3-v3": "^3.29",
        "livewire/livewire": "^3.6.4",
        "mallardduck/blade-lucide-icons": "^1.23",
        "propaganistas/laravel-phone": "^5.3.6",
        "romanzipp/laravel-seo": "^2.9.0",
        "spatie/laravel-google-fonts": "^1.4.3",
        "verschuur/laravel-robotstxt": "^5.2"
    },
    "require-dev": {
        "ext-simplexml": "*",
        "barryvdh/laravel-debugbar": "^3.15.4",
        "fakerphp/faker": "^1.24.1",
        "larastan/larastan": "^3.5",
        "laravel/pail": "^1.2.3",
        "laravel/pint": "^1.24",
        "laravel/sail": "^1.43.1",
        "mockery/mockery": "^1.6.12",
        "nunomaduro/collision": "^8.8.2",
        "pestphp/pest": "^3.8.2",
        "pestphp/pest-plugin-laravel": "^3.2.0",
        "rector/rector": "^2.1.2",
        "soloterm/solo": "^0.4.0",
        "stillat/blade-parser": "^2.0",
        "tightenco/duster": "3.1"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan storage:link"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "bun concurrently -c \"#c4b5fd,#fb7185,#fdba74\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"bun run dev\" --names=queue,logs,vite"
        ],
        "test": [
            "@php artisan config:clear --ansi",
            "@php artisan test"
        ],
        "lint": [
            "./vendor/bin/duster fix"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
